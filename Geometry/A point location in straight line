#include <bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
    int tc;
    cin>>tc;
    while(tc--)
    {
        ll x1,y1,x2,y2,x3,y3;
        cin>>x1>>y1>>x2>>y2>>x3>>y3;
        x2-=x1,x3-=x1;
        y2-=y1,y3-=y1;
       ll c= x2 * y3 - y2 * x3;
        if(c == 0)cout<<"TOUCH"<<endl;
        else if(c >0)cout<<"LEFT"<<endl;
        else cout<<"RIGHT"<<endl;
    
    }
   
    return 0;
}
